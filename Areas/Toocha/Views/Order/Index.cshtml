            // Ẩn tất cả danh sách sản phẩm ban đầu
            $('.list-product').hide();
            
            // Xử lý click vào category-name
            $('.category-name').click(function() {
                var $categoryName = $(this);
                var $listProduct = $categoryName.siblings('.list-product');
                
                // Toggle hiển thị danh sách sản phẩm
                $listProduct.slideToggle();
                
                // Toggle icon
                $categoryName.find('i').toggleClass('fa-angle-down fa-angle-up');
            });
        });
